{
  "kind": "collectionType",
  "collectionName": "category_groups",
  "info": {
    "singularName": "category-group", // Must match exactly
    "pluralName": "category-groups",  // Must match exactly
    "displayName": "Category Group",
    "description": "Grouping categories for alphabet entries"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "KumpulanKategori": {
      "type": "string",
      "required": true
    },
    "GroupCategory": {
      "type": "string",
      "required": true
    },
    "Remark": {
      "type": "text"
    },
    
    "AlphabetEntries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::alphabet-entry.alphabet-entry",
      "mappedBy": "CategoryGroup"
    }
  }
}